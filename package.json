{
  "name": "redux-starter-ui",
  "version": "1.0.10",
  "description": "",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "modules.root": "./es",
  "jsnext:main": "./es/index.js",
  "sideEffects": false,
  "dependencies": {
    "babel-core": "^6.26.3",
    "bourbon": "^5.0.1",
    "bourbon-neat": "^3.0.0",
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.14",
    "@storybook/addon-links": "^4.0.0-alpha.14",
    "@storybook/addons": "^4.0.0-alpha.14",
    "@storybook/react": "^4.0.0-alpha.14",
    "babel-cli": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.4.1",
    "node-sass": "^4.9.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "npm run clean && npm run build:lib && npm run build:es",
    "build:lib": "babel src --out-dir lib --ignore __tests__",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore __tests__",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js -o dist/redux-starter-ui.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js -o dist/redux-starter-ui.min.js",
    "heroku-postbuild": "npm run build-storybook",
    "start": "node server.js",
    "clean": "rimraf $(cd src; ls) dist lib es",
    "postpublish": "git push --tags",
    "prepublishOnly": "npm run test:cov && npm run clean && npm run build",
    "validate": " npm run test:cov && npm run clean && npm run build",
    "test": "cross-env NODE_ENV=test jest --runInBand --config jest.config.json",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "ship-it": "npm publish --tag next"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wearepush/redux-starter-ui.git"
  },
  "author": "wearepush",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wearepush/redux-starter-ui/issues"
  },
  "homepage": "https://github.com/wearepush/redux-starter-ui#readme",
  "files": [
    "README.md",
    "es",
    "lib",
    "dist",
    "*.js.flow"
  ],
  "npmName": "redux-starter-ui",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
